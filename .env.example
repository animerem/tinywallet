# Keystore type to use: local, ledger, turnkey, or environment
KEYSTORE_TYPE=local

# Solana RPC URL
RPC_URL=https://api.mainnet-beta.solana.com

# Commitment level for transactions: processed, confirmed, or finalized
COMMITMENT=confirmed

# Turnkey-specific environment variables
BASE_URL=https://api.turnkey.com
API_PUBLIC_KEY=your_api_public_key
API_PRIVATE_KEY=your_api_private_key
ORGANIZATION_ID=your_organization_id
WALLET_ID=your_wallet_id

# Environment keystore-specific variable
ENV_PRIVATE_KEY=[your_private_key_array] 